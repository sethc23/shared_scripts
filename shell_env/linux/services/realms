
alias rw_enable='sudo systemctl enable /home/ub2/SERVER2/realms_wiki/realms_wiki.service; sudo systemctl daemon-reload'
alias rw_disable='sudo systemctl disable /home/ub2/SERVER2/realms_wiki/realms_wiki.service; sudo systemctl daemon-reload'
alias rw_start='sudo systemctl start realms_wiki.service'
alias rw_stop='sudo systemctl stop realms_wiki.service'