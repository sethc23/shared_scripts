
alias svc_fs_enable='sudo systemctl daemon-reload; sudo systemctl enable $SERV_HOME/file_server/RUN/file_server.service 2> /dev/null'
alias svc_fs_disable='sudo systemctl disable $SERV_HOME/file_server/RUN/file_server.service; sudo systemctl daemon-reload'
alias svc_fs_start='sudo systemctl start file_server.service'
alias svc_fs_stop='sudo systemctl stop file_server.service; sudo systemctl daemon-reload'
alias svc_fs_status='sudo systemctl status file_server.service'